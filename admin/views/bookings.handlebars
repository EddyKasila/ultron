    <div class="col-xs-11 col-sm-11 col-md-11 col-md-offset-1"  id="activity-items">
   <div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12">
      <div id="title-con">
       <ol class="breadcrumb navbar-right">
        <li class="active"><a href="/index">Home</a></li>
        <li class="active"><a href="/bookings">Bookings</a></li>
      </ol>
      <h2><i class="fa fa-ticket"></i>&nbsp;Dashboard: Bookings</h2>
    </div>
    </div>
    <div  class="col-xs-12 col-sm-12 col-md-12" id="contents">
     <h3>List of Movies</h3><hr>
      <div class="row">
         <div class="col-md-12" id="movie-table">
           <table class="table">
               {{#if err}}
                   <th class="col-md-offset-4"><i class="fa fa-exclamation-triangle fa-2x"></i>No data found!</th>
               {{else}}
                 <thead><th><strong>Id</strong></th><th><strong>Movie ID</strong></th><th><strong>Username</strong></th><th><strong>Quantity</strong></th><th><strong>Total Cost</strong></th><th><strong>Posted</strong></th><th></th></thead>
                   {{#each booking as |bookings|}}
                       <tbody>
                         <tr><td>{{bookings.id}}</td><td>{{bookings.movieid}}</td><td>{{bookings.user}}</td>
                         <td>{{bookings.quantity}}</td><td>{{bookings.totalcost}}</td><td>{{bookings.post}}</td><td><a href="bookings/:id">Delete</a></td></tr>
                       </tbody>
                   {{/each}}
               {{/if}}

               </table>

        </div>

     </div>


   </div>
</div>
